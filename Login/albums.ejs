<body>

	<div class="limiter">
		<div class="container-login1001" style="background-image: url('images/b1.jpg');">

				<form class="login1001-form validate-form" method="POST" action="/find_a">
					<span class="login1001-form-title p-b-20 p-t-20">
					ALBUMS
					</span>

					<div class="wrap-input1001 validate-input" data-validate = "Enter song name">
						<input class="input1001" type="text" name="AName" placeholder="Album name">
						<span class="focus-input100" ></span>
					</div>

						<button class="login1001-form-btn">
							<i class="fa fa-search f "></i>
						</button>



				</form>
        <div class="result r p-b-10 ">
<span class="result p-b-10 m-b-10"><%=errora%></span>
          <p class="result p-b-10 ">ID :  <%=aid%></p>
          <p class="result p-b-10 p-t-10">Name :  <%=aatitle%></p>
          <p class="result p-b-10 p-t-10">Copyright date :  <%=cpdt%></p>
          <p class="result p-b-10 p-t-10">Format :  <%=aformat%></p>
          <p class="result p-b-10 p-t-10">Producer :  <%=aproducer%></p>
          <p class="result p-b-10 p-t-10">Songs :  <table><tr>
            <th>Song</th>
            <th>Singer</th>
          </tr><% for (var i in astitle,aauthor) { %>
             <tr>
               <td><%= astitle[i] %></td>
               <td><%= aauthor[i] %></td>
             </tr>
          <% } %> </table></p>
        </div>
        <div class="act">
          <a href="/add_a" class="button login100-form-btn" style="display:inline;padding-left: 40px;padding-right: 20px;padding-top: 15px;margin:30px;min-width:120px;" >
	       Add
         </a>
         <a href="/update_a" class="button login100-form-btn" style="display:inline;padding-left: 30px;padding-right: 20px;padding-top: 15px;margin:30px;" >
